		<urlActionName>se</urlActionName>

		<columns>
			<dburl>jdbc:mysql://localhost:3306/thd?useJDBCCompliantTimezoneShift=true&amp;useLegacyDatetimeCode=false&amp;serverTimezone=UTC</dburl>
			<usrname>root</usrname>
			<usrpwd>123456</usrpwd>
			<dbtable>sys_dic_pub</dbtable>
			<dbtype>mysql</dbtype>
			<schema>thd</schema>
		</columns>
</config>
